<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="false" />

    <param name="robot_config" value="$(find atf_test)/config/robot_config.yaml" />
    <param name="test_name" value="ts1_t1" />
    <param name="test_config" value="test_1" />

    <node name="atf_recorder" pkg="atf_recorder" type="recorder_core.py" output="screen">
        <param name="test_config_file" value="$(find atf_test)/config/test_config.yaml" />
        <param name="bagfile_output" value="$(find atf_test)/data/" />
    </node>
    <node name="obstacle_distance_node" output="screen" pkg="atf_recorder_plugins" type="obstacle_distance_node"/>

    <test test-name="test_recording" pkg="atf_test" type="publish_tf.py" time-limit="300.0" />

</launch>
